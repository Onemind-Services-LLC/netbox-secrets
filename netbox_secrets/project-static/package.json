{
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@eslint/js": "^9.34.0",
    "cookie": "^1.0.2",
    "esbuild": "^0.25.9",
    "typescript-eslint": "^8.40.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/cookie": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "typescript": "^5.9.2"
  },
  "license": "Apache-2.0",
  "main": "dist/secrets.js",
  "name": "netbox_secrets",
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "bundle": "node bundle.js && cp -fv dist/secrets.js* ../static/netbox_secrets/",
    "bundle:scripts": "node bundle.js --scripts",
    "bundle:styles": "node bundle.js --styles",
    "format": "prettier -w src/**/*.ts",
    "validate": "yarn validate:types && yarn validate:lint",
    "validate:formatting": "prettier -c src/**/*.ts",
    "validate:lint": "eslint ./src/**/*.ts",
    "validate:types": "tsc --noEmit"
  },
  "version": "2.3.1"
}
